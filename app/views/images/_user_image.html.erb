<div class="content-group listing-description">
  <%= link_to user_image do %>
    <div class="listing-image">
      <%= image_tag user_image.avatar.url %>
    </div>
    <p><strong>Image:</strong><%= user_image.name %></p>
    <p><strong>description:</strong><%= user_image.description %></p>
  <% end %>
  <p><strong>posted by <%= link_to user_image.user.username, user_image.user %></p>
  <p>was created on <%= user_image.created_time %></p>
</div>
