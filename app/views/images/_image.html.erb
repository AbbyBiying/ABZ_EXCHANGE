<h3>Image name: <%= image.name %></h3>
<h4>Image description: <%= image.description %></h4>
<%= link_to image do %>
  <div class="image-show">
    <%= image_tag image.url, width:"200" %>
  </div>
<% end %>
<p>Upload by user: <i class="username"><%= link_to image.user.username, image.user %></i></p>

<p>This image was created at <%= image.created_time %></p>
