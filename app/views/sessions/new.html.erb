<h3>If you have an account, please sign in.</h3>
<%= form_for :session, url: session_path do |form| %>
  <div>
    <%= form.label :username%>
    <%= form.text_field :username %>
  </div>

  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>

  <div>
    <%= form.submit "Sign in" %>
  </div>
<% end %>

<canvas data-processing-sources=<%= asset_path 'ball.pde' %>></canvas>
