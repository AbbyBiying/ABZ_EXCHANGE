<h1>All the Users 所有用户：</h1>
<!DOCTYPE html>
<html>
  <head>
    <style type="text/css">
      html, body, #map-canvas {
        height: 200px;
      }
    </style>
    <script type="text/javascript"
      src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCoT4I4sreJE9IESy6BmxGmvIY6o53bN10">
    </script>
    <script type="text/javascript">
      function initialize() {
        var mapOptions = {
          center: { lat: <%= current_user.latitude %>, lng: <%= current_user.longitude %>},
          mapTypeId: google.maps.MapTypeId.ROADMAP,
          zoom: 5,
        };
        var map = new google.maps.Map(document.getElementById('map-canvas'),
            mapOptions);
      }

      function autoRotate() {
        // Determine if we're showing aerial imagery
        if (map.getTilt() != 0) {
          map.setHeading(180);
          setTimeout('map.setHeading(270)',3000);
          setTimeout('map.setHeading(0)',6000);
          setTimeout('map.setHeading(90)',9000);
        }
      }
      google.maps.event.addDomListener(window, 'load', initialize);
    </script>
  </head>
  <body>
    <div id="map-canvas"></div>
  </body>
</html>


<%= render @user %>
<%= render "application/footer" %>
